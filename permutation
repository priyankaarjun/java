import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String s = sc.nextLine();
        char ch[] = s.toCharArray();
        int l = s.length();
        for (int i = 0; i <= l; i++) {
            for (int j = i+1; j <=l; j++) {
                System.out.print(s.substring(i,j) +" ");
            }
        }
    }
}
