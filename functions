import java.util.*;
class Demo{
    public static void greetings()
    {
        System.out.println("Welcome");
    }
    public static void main(String args[])
    {
        greetings();
    }
}

or


import java.util.*;
class Mobile{
    public static void iphone()
    {
        System.out.println("This is iphone");
    }
    public static void samsung()
    {
        System.out.println("This is samsung");
    }
    public static void realme()
    {
        System.out.println("This is realme");
    }
    public static void main(String args[])
    {
        Mobile ob1 = new Mobile();
        ob1.samsung();
    }
}


and


import java.util.*;
class Add{
    int a = 10;
    int b = 20;
    
    void sum()
    {
        System.out.println(a+b);
    }
    public static void main(String args[])
    {
        Add ob = new Add();
        ob.sum();
    }
}


and


import java.util.*;
class Total{
    int apple_price = 20;
    int apple_count = 5;
    
    void total_money()
    {
        System.out.println(apple_price*apple_count);
    }
    public static void main(String args[])
    {
        Total ob = new Total();
        ob.total_money();
    }
}
