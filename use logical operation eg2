import java.util.Scanner;
class Main {
    public static void main(String[] args) 
    {
        Scanner scan = new Scanner(System.in);
        int n1 = scan.nextInt();
        if(n1%2 == 0)
        {
            System.out.println("even number");
        }
        else
        {
            System.out.println("odd number");
        }
    }
}
