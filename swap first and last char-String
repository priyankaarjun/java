import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	    String s=sc.nextLine();
	    char ch[]=s.toCharArray();
	    int l=s.length();
	    char t;
	    if(ch[0]!=ch[l-1]){
	        t=ch[0];
	        ch[0]=ch[l-1];
	        ch[l-1]=t;
	    }
	    System.out.println(new String(ch));
	}
}

