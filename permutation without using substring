import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	    String str=sc.nextLine();
	    int l=str.length();
	    for(int i=0;i<l;i++){
	        for(int j=i+1;j<=l;j++){
	          for(int s=i;s<j;s++){
	              System.out.print(str.charAt(s));
	          }  
	          System.out.println();
	        }
	    }
	}
}
