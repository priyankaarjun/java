import java.util.Scanner;
class Main {
    public static void main(String[] args) 
    {
        boolean icecream = true;
        boolean hungry = true;
        if(icecream && hungry)
        {
            System.out.println("Eat");
        }
        else
        {
            System.out.println("Not eat");
        }
    }
}

O/P  : True

and


import java.util.Scanner;
class Main {
    public static void main(String[] args) 
    {
        boolean icecream = true;
        boolean hungry = false;
        if(icecream || hungry)
        {
            System.out.println("Eat");
        }
        else
        {
            System.out.println("Not eat");
        }
    }
}

O/P  :True
