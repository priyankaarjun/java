import java.util.*;
public class Main
{
	public static void main(String[] args) 
	{
	    Scanner sc = new Scanner(System.in);
	    String name = sc.nextLine();
	    char ch[] = name.toCharArray();
	    int len = name.length();
	    for(int i=0;i<len;i++)
	    {
	        if(ch[i]>='A' && ch[i]<='Z')
	        {
	            ch[i] = (char)(((int)ch[i])+32);
	        }
	    }
	    String result = new String(ch);
	    System.out.println(result);
	}
}
